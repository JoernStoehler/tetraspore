{
  "telemetry": {
    "enabled": true,
    "target": "local",
    "otlpEndpoint": "${OTEL_EXPORTER_OTLP_ENDPOINT}",
    "logPrompts": false
  },
  "logging": {
    "level": "info",
    "structured": true
  },
  "mcp": {
    "servers": {
      "tavily": {
        "enabled": true,
        "apiKey": "${TAVILY_API_KEY}"
      },
      "context7": {
        "enabled": true
      },
      "playwright": {
        "enabled": true
      }
    }
  },
  "models": {
    "default": "gemini-2.5-pro",
    "aliases": {
      "gemini": "gemini-2.5-pro",
      "gemini-pro": "gemini-2.5-pro",
      "flash": "gemini-2.5-flash",
      "gemini-flash": "gemini-2.5-flash"
    }
  },
  "workspace": {
    "autoDetect": true,
    "excludePatterns": [
      "**/node_modules/**",
      "**/.git/**",
      "**/dist/**",
      "**/build/**"
    ]
  }
}
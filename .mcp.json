{
  "mcpServers": {
    "tavily": {
      "command": "npx",
      "args": ["-y", "tavily-mcp"],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "context7-mcp"],
      "env": {}
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "puppeteer-mcp"],
      "env": {}
    }
  }
}